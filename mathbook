\begin{multicols}{2}
\begin{itemize}
    \item[A)] $y = 10x + 95$
    \item[B)] $y = x + 105$
    \item[C)] $y = x + 10$
    \item[D)] $y = 10x$
\end{itemize}
\end{multicols}

\vspace{0.2cm}   \vspace{3cm} \question \textbf{[June US 2025]}\saveanswer{B}\\ \\
