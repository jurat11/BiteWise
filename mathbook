            \node[anchor=north] at (2.85, 2.55) {$d^\circ$}; % Label x
            \node[anchor=west] at (0.55, 2.3) {$g^\circ$}; % Label y
            \node[anchor=north west] at (1.05, -0.82) {$p^\circ$}; % Label z
