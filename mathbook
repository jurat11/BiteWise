       \vspace{3cm}  \question \textbf{[December 2024]}\saveanswer{9600} \\ 
    $$
            \begin{tikzpicture}
        % Irregular shape (Approximated with Bezier curves)
            \draw[thick] (-2,3) 
                to[out=90, in=180] (-1.6,3.9) 
                to[out=0, in=120] (-0.8,3.7) 
                to[out=-60, in=100] (-1,3) 
                to[out=-100, in=30] (-1.5,1.5) 
                to[out=200, in=-90] (-2,3);
            % Vertical segment inside the shape (x feet)
            \draw[thick] (-1.5,3.9) -- (-1.5,1.5) node[midway, left] {\text{$x$ feet}};            
            % Label points inside the shape
            \node[above] at (-1.5,3.9) {\textbf{P}};
            \node[below] at (-1.5,1.4) {\textbf{T}};            
            % Large triangle (PQT)
            \draw[thick] (-1.5,3.9) -- (2,2) -- (-1.5,1.5) -- cycle;
            \node[right] at (2,2.3) {\textbf{Q}};            
            % Smaller triangle (QRS)
            \draw[thick] (2,2) -- (4,2.5) -- (4,1) -- cycle;
            \node[right] at (4,2.5) {\textbf{S}};
            \node[below] at (4.1,1) {\textbf{R}};            
            % Add note
            \node[below] at (1,0.5) {\text{Note: Figure not drawn to scale.}};            
        \end{tikzpicture}$$
