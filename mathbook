    \item[A)] $-3$ only
    \item[B)] $-3$ or $3$
    \item[C)] $0$ only
    \item[D)] $3$ only
\end{itemize}
\end{multicols}

    \vspace{0.2cm}   \vspace{4cm} \question \textbf{[August 2024]}\saveanswer{C} \\ 
