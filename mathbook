            \draw[thick, line width=1.5pt] (-4,0) -- (0,0) -- (0, -2) -- (-4, -2) -- cycle;
            \node at (0.3,0) {$X$};
            \node at (-4.3,0) {$W$};
            \node at (0.3,-2.3) {$Y$};
            \node at (-4.3,-2.3) {$Z$};
        \end{tikzpicture}
        $$ $$
        \begin{tikzpicture}
            \node at (0, -1) {Note: Figure not drawn to scale};
        \end{tikzpicture}
        $$
