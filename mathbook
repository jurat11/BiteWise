        \begin{multicols}{2}
            \begin{itemize}
                \item[A)] {\large ${\frac{71}{5}}$}
                \item[B)] {\large ${\frac{71}{2}}$}
                \item[C)] {\large ${\frac{213}{5}}$} 
                \item[D)] 71
            \end{itemize}
        \end{multicols}


