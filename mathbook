            \draw[thick] (2,0) -- (2,-2);
            \draw[dashed] (0,-2) ellipse (2 and 0.6);
            \draw[thick] (-2,-2) arc (180:360:2 and 0.6);
            \draw[dashed] (2,0) arc (0:180:2 and 0.6);
            \draw[thick] (0,0) -- (2,0);
