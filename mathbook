        \begin{multicols}{4}
            \begin{itemize}
                \item[A)] Data set A
                \item[B)] Data set B
                \item[C)] Data set C
                \item[D)] Data set D
            \end{itemize}
        \end{multicols}


\vspace{0.2cm}   \vspace{5cm} \question \textbf{[August 2024]}\saveanswer{A}\\ 
\begin{center}
    \begin{tabular}{|c|c|c|}
        \hline
        \textbf{Value} & \textbf{Data set $A$ frequency} & \textbf{Data set $B$ frequency} \\ 
        \hline
        40 & 2 & 8 \\ 
        \hline
        43 & 4 & 7 \\ 
        \hline
        46 & 5 & 5 \\ 
        \hline
        49 & 7 & 4 \\  
        \hline
        52 & 8 & 2 \\ 
        \hline
    \end{tabular}
