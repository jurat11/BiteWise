\subsection*{Geometry \& Trigonometry Formulas}
\addcontentsline{toc}{subsection}{Geometry \& Trigonometry Formulas}



\begin{multicols}{2}
\begin{center}
    {\large\textbf{Triangles}}
\end{center}
\begin{itemize}
        \item[\textcolor{RAL3003}{\textbf{•}}] The sum of the measures in degrees of the angles of a triangle is \textcolor{RAL3003}{$180^\circ$}.
\end{itemize}
\begin{center}
    \begin{tabular}{m{4cm} p{7cm}} % Left-align text in column 2
        \begin{tikzpicture}[scale=0.75]
            % Draw the triangle
            \draw[thick,line width=1.5pt] (0,0) -- (4,0) -- (2,3) -- cycle;
            
            % Label the angles
            \node at (0.65,0.4) {$a^\circ$};
            \node at (3.35,0.4) {$c^\circ$};
            \node at (2.05,2.35) {$b^\circ$};
            
            % Extend the base line and mark external angle
            \draw[thick,line width=1.5pt] (4,0) -- (6,0);
            \node at (4.4,0.4) {$x^\circ$};
        \end{tikzpicture}  
        &
        \raggedright \vspace{-1.7cm}
        \hspace{-0.5cm}An \textbf{\textcolor{RAL3003}{exterior angle}} is \\
        \hspace{-0.5cm}always equal to the sum \\
        \hspace{-0.5cm}of the two angles in the \\
        \hspace{-0.5cm}triangle furthest from it. \\ 
        \hspace{-0.5cm}In this case, \textcolor{RAL3003}{$x = a + b$}.
    \end{tabular}
\end{center}

\begin{center}
    \begin{tabular}{cc}
    \begin{tikzpicture}[scale=1]
         % Draw the triangle
        \draw[thick, line width=1.5pt] (0,0) -- (4,0) -- (1,3) -- cycle;
        
        % Label the base
        \draw[thick,<->] (0,-0.2) -- (4,-0.2);
        \node at (2,-0.5) {\( b \)};
        
        % Draw the height
        \draw[thick,dashed] (1,0) -- (1,3);
        \node at (1.3,1.5) {\( h \)};
    
        % Right angle marker
        \draw (1,0.25) -- (1.25,0.25) -- (1.25,0);
        \node[below] at (2,-1) {$A = \frac{1}{2}hb$};
    \end{tikzpicture}
    &
    \begin{tikzpicture}[scale=1]
        % Draw the triangle
        \draw[thick, line width=1.5pt] (0,0) -- (4,0) -- (4,3) -- cycle;
        
        % Label the sides
        \node at (2,-0.3) {\( a \)};
        \node at (4.3,1.5) {\( b \)};
        \node at (2.2,2.2) {\( c \)};
        
        % Right angle marker
        \draw (3.8,0) -- (3.8,0.2) -- (4,0.2);
        \node[below] at (2,-1) {$c^2=a^2+b^2$};
        %\node[below] at (2,-2.4) {  };
    \end{tikzpicture}
    \end{tabular}
\end{center}
\begin{center}
    {\large\textbf{Special Right Triangles}}
\end{center}
\begin{center}
    \begin{tabular}{cc}
        \begin{tikzpicture}
            % 30-60-90 Triangle
            \draw[thick, line width=1.5pt] (1,0) -- (3,0) -- (3,3.46) -- cycle;
            \node at (2,-0.3) {\( x \)};
            \node at (3.5,1.3) {\( x\sqrt{3} \)};
            \node at (1.6,1.78) {\( 2x \)};
            \node at (2.7,2.2) {\( 30^\circ \)};
            \node at (1.5,0.2) {\( 60^\circ \)};
            \draw (3,0.25) -- (2.75,0.25) -- (2.75,0);
        \end{tikzpicture}
         &   
        \begin{tikzpicture}
            % 45-45-90 Triangle
            \draw[thick, line width=1.5pt] (5,0) -- (8,0) -- (5,3) -- cycle;
            \node at (6.5,-0.3) {\( s \)};
            \node at (4.7,1.5) {\( s \)};
            \node at (7,1.6) {\( s\sqrt{2} \)};
            \node at (5.35,2.2) {\( 45^\circ \)};
            \node at (7.35,0.25) {\( 45^\circ \)};
            \draw (5,0) -- (5.25,0) -- (5.25,0.25) -- (5,0.25) -- cycle;    
        \end{tikzpicture}
    \end{tabular}
\end{center}
\begin{center}
{\large\textbf{Trigonometry}}
\end{center}
\begin{center}
        \begin{tikzpicture}[scale=0.4]
            % Draw the triangle
            \draw[thick, line width = 1.5pt] (0,0) -- (12,0) -- (12,5) -- cycle;
            
            % Label the sides
            \node[below] at (6,0) {12};
            \node[right] at (12,2.5) {5};
            \node[above left] at (6,2.5) {13};
            
            % Mark the right angle
            \draw (12,0) -- ++(-0.5,0) -- ++(0,0.5) -- ++(0.5,0);
            
            \node[right] at (1.4,0.55) {$x^\circ$};
        \end{tikzpicture}
\end{center}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}]${\sin x}=\dfrac{\text{opposite}}{\text{hypotenuse}}=\dfrac{5}{13}$
         
    \item[\textcolor{RAL3003}{\textbf{•}}]${\cos x}=\dfrac{\text{adjacent}}{\text{hypotenuse}}=\dfrac{12}{13}$
         
    \item[\textcolor{RAL3003}{\textbf{•}}]${\tan x}=\dfrac{\text{opposite}}{\text{adjacent}}=\dfrac{5}{12}$
    \item[\textcolor{RAL3003}{\textbf{•}}]{\boldmath$\sin x=\cos(\dfrac{\pi}{2}-x)$
    \item[\textcolor{RAL3003}{\textbf{•}}] $\cos x=\sin(\dfrac{\pi}{2}-x)$}
    \item[\textcolor{RAL3003}{\textbf{•}}] $\cos x=\sin y$, when $x+y=90^\circ$
\end{itemize}
\begin{multicols*}{2}
\begin{center}
    \begin{tikzpicture}[line width=1.5pt]
        % Define points
        \coordinate (C) at (0,0);    % Bottom-left vertex
        \coordinate (B) at (3,0);    % Bottom-right vertex
        \coordinate (A) at (0,2);    % Right-angled vertex
        \coordinate (H) at (12/13,18/13);    % Foot of the height (same as A)

        % Draw triangle
        \draw[thick,line width=1.5pt] (A) -- (B) -- (C) -- cycle;

        % Draw height
        \draw[dashed] (C) -- (H);

        % Mark right angle at C
        \draw (C)++(0.2,0) --++(0,0.2) --++(-0.2,0);
        
        % Label sides
        \node[below] at (1.5,0) {\( a \)};
        \node[left] at (0,1) {\( b \)};
        %\node[above right] at (1,1.5) {\( c \)};
        \node[right] at (0.5,0.8) {\( h \)};
        \node[above] at (0.6,1.6) {\( c_b \)};
        \node[above] at (2,0.7) {\( c_a \)};

        % Mark points
        \filldraw (A) circle (1pt) node[above left] {\( A \)};
        \filldraw (B) circle (1pt) node[below right] {\( B \)};
        \filldraw (C) circle (1pt) node[below left] {\( C \)};
        \filldraw (H) circle (1pt) node[above right] {\( H \)};
    \end{tikzpicture}
\end{center}
\newcolumn
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] $h=\sqrt{c_a c_b}$
    \item[\textcolor{RAL3003}{\textbf{•}}] $h=\dfrac{ab}{c}$
    \item[\textcolor{RAL3003}{\textbf{•}}] $CH-$height=$h$
    \item[\textcolor{RAL3003}{\textbf{•}}] $AH=c_b$
    \item[\textcolor{RAL3003}{\textbf{•}}] $HB=c_a$   
\end{itemize}

\end{multicols*}

\end{multicols}


\newpage
\begin{center}
    {\large\textbf{Similarity\&Congruency}}
\end{center}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] Two figures (such as triangles) are \textbf{similar ($\sim$)} if they have the same shape, regardless of their size. $\triangle ABC \sim \triangle MNK$
\end{itemize}
        $$
        \begin{tikzpicture}[scale=1]
            % First Triangle: vertices A, B, C
            \coordinate (A) at (0,0);
            \coordinate (C) at (3,0);
            \coordinate (B) at (1.5,2);
            
            % Draw first triangle
            \draw[line width=1.5pt] (A) -- (B) -- (C) -- cycle;
            
            % Label vertices
            \node[below left] at (A) {\( A \)};
            \node[below right] at (C) {\( C \)};
            \node[above] at (B) {\( B \)};
        
            
            \node[above right] at (0.15,0) {\( \alpha \)};
            \node[below] at (1.45,1.9) {\( \beta \)};
            \node[above left] at (2.7,0) {\( \gamma \)};
            % Label sides
            \node[below] at (1.5,0) {\( c_1 \)};
            \node[right] at (2.25,1) {\( b_1 \)};
            \node[left] at (0.75,1) {\( a_1 \)};
            
            % Mark angle ABC with an arc and label \alpha
            
            % Second Triangle: vertices M, N, K
            \coordinate (M) at (5,0);
            \coordinate (K) at (8,0);
            \coordinate (N) at (6.5,2.5);
            \node[above right] at (5.15,0) {\( \alpha \)};
            \node[below] at (6.45,2.35) {\( \beta \)};
            \node[above left] at (7.7,0) {\( \gamma \)};
            
            % Draw second triangle
            \draw[line width=1.5pt] (M) -- (N) -- (K) -- cycle;
            
            % Label vertices
            \node[below left] at (M) {\( M \)};
            \node[below right] at (K) {\( K \)};
            \node[above] at (N) {\( N \)};
            
            % Label sides
            \node[below] at (6.5,0) {\( c_2 \)};
            \node[right] at (7.25,1.25) {\( b_2 \)};
            \node[left] at (5.75,1.25) {\( a_2 \)};
        \end{tikzpicture}$$
        $$\dfrac{a_1}{a_2}=\dfrac{b_1}{b_2}=\dfrac{c_1}{c_2}=\dfrac{p_1}{p_2}=\dfrac{h_1}{h_2},$$
        $$\left(\dfrac{a_1}{a_2}\right)^2=\dfrac{A_1}{A_2},$$
        $$\text{If two 3D shapes are similar: } \left(\dfrac{a_1}{a_2}\right)^3=\dfrac{V_1}{V_2}. $$
    \begin{itemize}
        \item[\textcolor{RAL3003}{\textbf{•}}] Two figures are \textbf{congruent ($\cong$)} if they have exactly the same size and shape. This means that all corresponding sides and angles in the figures are equal.
    \end{itemize}
    $$
        \begin{tikzpicture}[scale=1]
            % First Triangle: vertices A, B, C
            \coordinate (A) at (0,0);
            \coordinate (C) at (3,0);
            \coordinate (B) at (1.5,2);
            
            % Draw first triangle
            \draw[line width=1.5pt] (A) -- (B) -- (C) -- cycle;
            
            % Label vertices
            \node[below left] at (A) {\( A \)};
            \node[below right] at (C) {\( C \)};
            \node[above] at (B) {\( B \)};
        
            
            \node[above right] at (0.15,0) {\( \alpha \)};
            \node[below] at (1.45,1.9) {\( \beta \)};
            \node[above left] at (2.7,0) {\( \gamma \)};
            % Label sides
            \node[below] at (1.5,0) {\( c \)};
            \node[right] at (2.25,1) {\( b \)};
            \node[left] at (0.75,1) {\( a \)};
            
            % Mark angle ABC with an arc and label \alpha
            
            % Second Triangle: vertices M, N, K
            \coordinate (M) at (5,0);
            \coordinate (K) at (8,0);
            \coordinate (N) at (6.5,2);
            \node[above right] at (5.15,0) {\( \alpha \)};
            \node[below] at (6.45,1.9) {\( \beta \)};
            \node[above left] at (7.7,0) {\( \gamma \)};
            
            % Draw second triangle
            \draw[line width=1.5pt] (M) -- (N) -- (K) -- cycle;
            
            % Label vertices
            \node[below left] at (M) {\( D \)};
            \node[below right] at (K) {\( F \)};
            \node[above] at (N) {\( E \)};
            
            % Label sides
            \node[below] at (6.5,0) {\( c \)};
            \node[right] at (7.25,1) {\( b \)};
            \node[left] at (5.75,1) {\( a \)};
        \end{tikzpicture}$$
        
    \begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{SSS (Side-Side-Side):} \\
    If all three sides of one triangle are equal to the corresponding three sides of another triangle, then the triangles are congruent.
    $$\triangle ABC \cong \triangle DEF$$
    $\textbf{ When: }  AB = DE,\  BC = EF,\  CA = FD.$  
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{SAS (Side-Angle-Side):} \\
    If two sides and the included angle of one triangle are equal to two sides and the included angle of another triangle, then the triangles are congruent.
    $$\triangle ABC \cong \triangle DEF \quad$$
    $\textbf{When: } AB = DE,\quad \angle B = \angle E,\quad BC = EF.$
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{ASA (Angle-Side-Angle):} \\
    If two angles and the included side of one triangle are equal to two angles and the included side of another triangle, then the triangles are congruent.
    $$\triangle ABC \cong \triangle DEF $$
    $\textbf{When: }  \angle A = \angle D,\quad AB = DE,\quad \angle B = \angle E.$
    
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{AAS (Angle-Angle-Side) or SAA (Side-Angle-Angle):} \\
    If two angles and a non-included side of one triangle are equal to the corresponding two angles and side of another triangle, then the triangles are congruent.
    $$ \triangle ABC \cong \triangle DEF$$
    $\textbf{When: }\angle A = \angle D,\quad \angle B = \angle E,\quad BC = EF.$
    
\end{itemize}

\newpage



\begin{multicols}{2}

\begin{center}
    {\large\textbf{3D shapes}}
\end{center}
\begin{center}
    \begin{tabular}{cc}
        \begin{tikzpicture}[scale=1]
            \draw[line width=1.5pt] (0,0) -- (4,0) -- (4,2) -- (0,2) -- cycle; % Base
            \draw[line width=1.5pt] (1,1) -- (1,3) -- (5,3) -- (5,1); % Front left edge
            \draw[line width=1.5pt] (4,0) -- (5,1) -- (1,1) -- (0,0); % Front right edge
            \draw[line width=1.5pt] (4,2) -- (5,3) -- (1,3) -- (0,2); % Back edge
            
            % Labels
            \node[below] at (2,0) {$\ell$};
            \node[right] at (4,1.4) {$h$};
            \node[right] at (4.4,0.4) {$w$};
            \node[below] at (2,-1) {$V=\ell hw$};
        \end{tikzpicture}
        &
        \begin{tikzpicture}[scale=1]
            % Draw the cylinder
            \draw[thick,line width=1.5pt] (0.5,0) ellipse (1.5 and 0.5); % Top ellipse
            \draw[thick,line width=1.5pt] (-1,0) -- (-1,-2); % Left side
            \draw[thick] (2,0) -- (2,-2); % Right side
            \draw[thick,line width=1.5pt] (-1,-2) arc[start angle=180, end angle=360, x radius=1.5, y radius=0.5]; % Bottom ellipse front
            \draw[dashed,line width=1.5pt] (-1,-2) arc[start angle=180, end angle=0, x radius=1.5, y radius=0.5]; % Bottom ellipse back
            
            % Add labels
            \draw[thick,line width=1.5pt] (0.5,0) -- (-1,0);
            \node[above] at (-0.25,-0.1) {$r$};
            \draw[thick,line width=1.5pt] (2,-2) -- (2,0) node[midway, right] {$h$}; % Height
            \node[below] at (0.7,-3.4) {$V=h\pi r^2$};
        \end{tikzpicture}  
    \end{tabular}
\end{center}

\begin{center}
    \begin{tabular}{cc}
        \begin{tikzpicture}[line width=1.5pt]
            \begin{scope}[scale=1] % Scaling the sphere
                % Draw the outer circle (sphere)
                \draw[thick, line width=1.5pt] (0,0) circle(1.5); % Outer sphere
                
                % Draw the horizontal diameter
                \draw[dashed] (-1.5,0) -- (1.5,0); % Horizontal diameter
                
                % Draw the vertical radius
                \draw[thick,line width=1.5pt ,->] (0,0) -- (0,1.5) node[midway, right] {$r$}; % Vertical radius
                
                % Draw an ellipse to show the perspective
                \draw[dashed] (0,0) ellipse (1.5 and 0.3); % Horizontal ellipse (perspective)
                \node[below] at (0,-2) {$V=\frac{4}{3}\pi r^3$};
            \end{scope}
        \end{tikzpicture}

        \begin{tikzpicture}[line width=1.5pt]
            \begin{scope}[scale=1] % Scaling the cone
                % Draw the base of the cone
                \draw[thick,line width=1.5pt] (-1,0) arc[start angle=180, end angle=360,x radius=1.5, y radius=0.5 ]; % Front half of the ellipse
                \draw[dashed,line width=1.5pt] (-1,0) arc[start angle=180, end angle=0,x radius=1.5, y radius=0.5]; % Back half of the ellipse
                
                % Draw the sides of the cone
                \draw[thick,line width=1.5pt] (-1,0) -- (0.5,3); % Left side
                \draw[thick,line width=1.5pt] (2,0) -- (0.5,3); % Right side
                
                % Add labels for radius and height
                \draw[thick,line width=1.5pt] (0.5,0) -- (0.5,3) node[midway, right] {$h$}; % Height
                \draw[thick,line width=1.5pt] (0.5,0) -- (2,0); % Radius
                \draw[thick] (0.5,0.25) -- (0.75,0.25) -- (0.75,0) ;
                \node[above] at (1.2,-0.1) {$r$};
                \node[below] at (0.5,-1) {$V=\frac{1}{3}\pi r^2h$};
            \end{scope}
        \end{tikzpicture}

        
    \end{tabular}
    \begin{tikzpicture}[line width=1.5pt]
            \begin{scope}[scale=1] % Scaling the pyramid
                % Base square (dashed for back edges, solid for front edges)
                \draw[dashed,line width=1.5pt] (0,0) -- (3,0); % Back top edge
                \draw[thick,line width=1.5pt] (3,0) -- (2,-1) node[midway,right] {$\ell$}; % Back right edge
                \draw[thick,line width=1.5pt] (2,-1) -- (-1,-1) node[midway,below] {$w$}; % Front bottom edge
                \draw[dashed,line width=1.5pt] (-1,-1) -- (0,0); % Front left edge
                
                % Apex of the pyramid
                \coordinate (A) at (0.5,2); % Apex of the pyramid
                
                % Connect the apex to the vertices of the base
                \draw[dashed,line width=1.5pt] (A) -- (0,0); % Apex to top-left
                \draw[thick,line width=1.5pt] (A) -- (3,0); % Apex to top-right
                \draw[thick,line width=1.5pt] (A) -- (2,-1); % Apex to bottom-right
                \draw[thick,line width=1.5pt] (A) -- (-1,-1); % Apex to bottom-left
                \draw[thick,line width=1.5pt] (A) -- (0.5,-0.5) node[midway, right] {$h$};
                \draw[thick] (0.5,-0.25)-- (0.75,-0.25) -- (0.75,-0.5);
                \node[below] at (1,-1.7) {$V=\frac{1}{3}wlh$};
            \end{scope}
        \end{tikzpicture}
\end{center}
    
\begin{center}
    {\large\textbf{Circle}}
\end{center}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] The number of degrees of arc in a circle is \textcolor{RAL3003}{$360^\circ$}.
    \item[\textcolor{RAL3003}{\textbf{•}}] The number of radians of arc in a circle is \textcolor{RAL3003}{\( 2\pi \)}.
\end{itemize}


\begin{center}
        \begin{tikzpicture}[scale=1]
            % Draw the circle
            \draw[thick, line width=1.5pt] (0,0) circle(2);
                
            % Draw the radius lines
            \draw[thick, line width=1.5pt] (0,0) -- (60:2); % Line to A (60 degrees)
            \draw[thick, line width=1.5pt] (0,0) -- (0:2);  % Line to B (0 degrees)
            \draw[thick, line width=1.5pt] (210:2) -- (60:2);
            \draw[thick, line width=1.5pt] (210:2) -- (0:2);
            % Label points
            \node[above] at (60:2) {$A$}; % Point A
            \node[right] at (0:2) {$B$};  % Point B    
            \node[left] at (210:2) {$C$};
            \node[below] at (0,0) {$O$};
            % Label the angles
            \node[above right] at (30:0.3) {$60^\circ$}; % Interior angle
            \node[right] at (30:2) {$60^\circ$};       % Exterior angle
            \node[above right] at (212:1.4) {$30^\circ$};  
            % Mark the interior angle
            \draw[thick, line width=1.2pt] (0.3,0) arc[start angle=0, end angle=60, radius=0.3];
        \end{tikzpicture}  
\end{center}

\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] $OA-$radius=$r$
    \item[\textcolor{RAL3003}{\textbf{•}}] $\angle AOB-$central angle
    \item[\textcolor{RAL3003}{\textbf{•}}] $\angle ACB-$inscribed angle
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Arc length: } \(\dfrac{\theta}{360}\cdot 2\pi r\) 
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Area of sector: } {\(\dfrac{\theta}{360}\cdot \pi r^2\)} \\ \ \\ \textbf{$\theta-$} central angle in degrees.
\end{itemize}
$$
    \begin{tikzpicture}[scale=1.2]
        % Draw axes
        \draw[->] (-1,0) -- (3,0) node[anchor=north] {\(x\)};
        \draw[->] (0,-1) -- (0,3) node[anchor=east] {\(y\)};
        
        % Define circle parameters
        \def\a{1}  % x-coordinate of center
        \def\b{1}  % y-coordinate of center
        \def\r{1.5}  % radius

        % Draw circle
        \draw[thick, line width=1.5pt] (\a,\b) circle (\r);
        
        % Mark center
        \filldraw (\a,\b) circle (1.5pt) node[anchor=north] {\((a,b)\)};
        
        % Draw radius line
        \draw[dashed] (\a,\b) -- (\a+\r,\b) node[midway, above] {\(r\)};
        
    \end{tikzpicture}
$$  
        \textbf{$(x-a)^2+(y-b)^2 = r^2 $}
        \begin{itemize}
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{$r-$}radius
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{$(a,b)-$}center coordinate of circle
        \end{itemize}
\end{multicols}
\begin{multicols}{2}
    \begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] Sum of interior angles=$180^\circ(n-2)$
    \item[\textcolor{RAL3003}{\textbf{•}}] Sum of exterior angles=$360^\circ$
\end{itemize}
\end{multicols}

