\subsection*{Advanced Math Formulas}
\addcontentsline{toc}{subsection}{Advanced math Formulas}




\subsubsection*{Quadratic Functions-Forms}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Standard Form:} $y=ax^2+bx+c,$ where:
    \begin{itemize}
        \item[\textcolor{RAL3003}{\textbf{•}}] $c-$$y$-intercept.
    \end{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Factored Form:} $y=a(x-x_1)(x-x_2),$ where:
    \begin{itemize}
        \item[\textcolor{RAL3003}{\textbf{•}}] $x_1,x_2-$ $x$-intercepts.
    \end{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Vertex Form:} $y=a(x-h)^2+k$, where:
    \begin{itemize}
        \item[\textcolor{RAL3003}{\textbf{•}}]\textbf{($h, k$)}-vertex coordinate.
    \end{itemize}
\end{itemize}
$$
    \begin{tikzpicture}[scale=0.9]
    \begin{axis}[
        axis lines=middle,
        xmin=-5, xmax=6,
        ymin=-5, ymax=6,
        xtick={-5,-4,...,6},
        ytick={-5,-4,...,6},
        xlabel={$x$},
        ylabel={$y$},
        grid=none,
        enlargelimits=true,
        width = 12cm, % Width of the plot
        height = 12cm, % Height of the plot
        every axis x label/.style={at={(ticklabel* cs:1)},anchor=north},
        every axis y label/.style={at={(ticklabel* cs:1)},anchor=east}
    ]
    
    % Plot the parabola
    \addplot[
        domain=-2.1:4.1,
        samples=100,
        thick,
        line width=1.5pt,
        RAL3003, <->
    ] {x^2-2*x-3};
    
    % Highlight points
    \addplot[only marks, mark=*] coordinates {(-1, 0) (3, 0)}; % x-intercepts
    \addplot[only marks, mark=*] coordinates {(0, -3)};       % y-intercept
    \addplot[only marks, mark=*] coordinates {(1, -4)};       % vertex

    % Axis of symmetry
    \draw [dashed, thick, brown] (axis cs:1,-5.5) -- (axis cs:1,6);

    % Annotations
    \node at (axis cs:1, 6.3) [anchor=west] {\textbf{Axis of Symmetry}};
    \node at (axis cs:4, -4.2) [anchor=north] {\textbf{Vertex}};
    \node at (axis cs:-5, -4) [anchor=west] {\textbf{$y$-intercept}};
    \node at (axis cs:1.8, 2) [anchor=south] {\textbf{$x$-intercepts}};

    % Arrow pointing to vertex
    \draw[->] (axis cs:3, -4.5) -- (axis cs:1, -4);

    % Arrow pointing to y-intercept
    \draw[->] (axis cs:-2, -4) -- (axis cs:0, -3);

    % Arrow pointing to x-intercepts
    \draw[->] (axis cs:1.5, 2) -- (axis cs:-1, 0);
    \draw[->] (axis cs:1.5, 2) -- (axis cs:3, 0);
    \draw[->] (axis cs:1, 5) -- (axis cs:2, 6);
    % Dots on parabola ends for appearance
    \addplot[only marks, mark=*] coordinates {(-2, 5) (4, 5)};

    \end{axis}
    
\end{tikzpicture}
$$
\textbf{Axis of symmetry and $x$-coordinate of vertex:} $-\dfrac{b}{2a}$
\subsubsection*{Graph Reflection Rules}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] $-f(x) \rightarrow$ Reflection about the \textbf{$x$-axis}
    \item[\textcolor{RAL3003}{\textbf{•}}] $f(-x) \rightarrow$ Reflection about the \textbf{$y$-axis}
\end{itemize}

\begin{center}
    \textbf{When } \boldmath$f(x) = x^3 + 1$
    \hspace{-1cm}
    \begin{tabular}{ccc} 
        \begin{tikzpicture}[scale=1]
            \begin{axis}[
                axis lines=middle,
                xlabel={$x$},
                ylabel={$y$},
                xtick=\empty,
                ytick=\empty,
                xmin=-4, xmax=3,
                ymin=-2, ymax=5,
                domain=-2:2,
                samples=100,
                smooth,
                width=4cm,
                height=4cm,
                title style = {font=\boldmath},
                title={$f(x)$}
                ]
                \addplot[RAL3003, line width=2pt] {x^3+1};
            \end{axis}
        \end{tikzpicture}&
        \begin{tikzpicture}[scale=1]
            \begin{axis}[
                axis lines=middle,
                xlabel={$x$},
                ylabel={$y$},
                xtick=\empty,
                ytick=\empty,
                xmin=-4, xmax=3,
                ymin=-2, ymax=5,
                domain=-2:2,
                samples=100,
                smooth,
                width=4cm,
                height=4cm,
                title style = {font=\boldmath},
                title={$f(-x)$}
                ]
                \addplot[RAL3003, line width=2pt] {-x^3+1};
                \addplot[line width=1.5pt,dashed] {x^3+1};
            \end{axis}
        \end{tikzpicture}&
        \begin{tikzpicture}[scale=1]
            \begin{axis}[
                axis lines=middle,
                xlabel={$x$},
                ylabel={$y$},
                xtick=\empty,
                ytick=\empty,
                xmin=-4, xmax=3,
                ymin=-3, ymax=3,
                domain=-2:2,
                samples=100,
                smooth,
                width=4cm,
                height=4cm,
                title style = {font=\boldmath},
                title={$-f(x)$}
                ]
                \addplot[RAL3003, line width=2pt] {-x^3-1};
                \addplot[line width=1.5pt,dashed] {x^3+1};
            \end{axis}
        \end{tikzpicture}
    \end{tabular}
\end{center}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Midpoint} of $A(x_1,y_1), B(x_2,y_2)$: 
     $$\left(\dfrac{x_1+x_2}{2}, \dfrac{y_1+y_2}{2} \right).$$
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Distance formula:}\( \sqrt{(x_2 - x_1)^2 + (y_2 - y_1)^2} \).
\end{itemize}
\subsubsection*{Function Translation Rules}
\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] $f(x + 1)$ \text{graph moves \textbf{LEFT} one.}
    \item[\textcolor{RAL3003}{\textbf{•}}] $f(x - 1)$ \text{graph moves \textbf{RIGHT} one.}
    \item[\textcolor{RAL3003}{\textbf{•}}] $f(x) + 1$ \text{graph moves \textbf{UP} one.}
    \item[\textcolor{RAL3003}{\textbf{•}}] $f(x) - 1$ \text{graph moves \textbf{DOWN} one.}
\end{itemize}
\begin{center}
\begin{tabular}{c|c}
    \begin{tikzpicture}[scale=1]
       \begin{axis}[
           axis lines=middle,
           xlabel={$x$},
           ylabel={$y$},
           xtick=\empty,
           ytick=\empty,
           xmin=-3, xmax=4,
           ymin=-2, ymax=5,
           domain=-1:3,
           samples=100,
           smooth,
           title style = {font=\boldmath},
           title={$y=f(x-1)$},
           width=4cm,
           height=4cm
       ]
           \addplot[RAL3003, line width=1.5pt] {(x-1)^2};
           \addplot[dashed, line width=1.5pt,domain=-2:2] {(x)^2};
       \end{axis}
   \end{tikzpicture}
   &
   \begin{tikzpicture}[scale=1]
       \begin{axis}[
           axis lines=middle,
           xlabel={$x$},
           ylabel={$y$},
           xtick=\empty,
           ytick=\empty,
           xmin=-4, xmax=3,
           ymin=-2, ymax=5,
           domain=-3:1,
           samples=100,
           smooth,
           title style = {font=\boldmath},
           title={$y=f(x+1)$},
           width=4cm,
           height=4cm
       ]
           \addplot[RAL3003, line width=1.5pt] {(x+1)^2};
           \addplot[dashed, line width=1.5pt,domain=-2:2] {(x)^2};
       \end{axis}
   \end{tikzpicture}
\end{tabular}
\end{center}

\begin{center}
\begin{tabular}{c|c}
   \begin{tikzpicture}[scale=1]
       \begin{axis}[
           axis lines=middle,
           xlabel={$x$},
           ylabel={$y$},
           xtick=\empty,
           ytick=\empty,
           xmin=-4, xmax=3,
           ymin=-2, ymax=5,
           domain=-2:2,
           samples=100,
           smooth,
           title style = {font=\boldmath},
           title={$y=f(x)-1$},
           width=4cm,
           height=4cm
       ]
           \addplot[RAL3003, line width=1.5pt] {x^2-1};
           \addplot[dashed, line width=1.5pt,domain=-2:2] {(x)^2};
       \end{axis}
   \end{tikzpicture}
   &
   \begin{tikzpicture}[scale=1]
       \begin{axis}[
           axis lines=middle,
           xlabel={$x$},
           ylabel={$y$},
           xtick=\empty,
           ytick=\empty,
           xmin=-4, xmax=3,
           ymin=-2, ymax=5,
           domain=-2:2,
           samples=100,
           smooth,
           title style = {font=\boldmath},
           title={$y=f(x)+1$},
           width=4cm,
           height=4cm
       ]
           \addplot[RAL3003, line width=1.5pt] {x^2+1};
           \addplot[dashed, line width=1.5pt,domain=-2:2] {(x)^2};
       \end{axis}
   \end{tikzpicture}
\end{tabular}
\end{center}



\subsubsection*{Graph Amplification Rules}
\textbf{$x$-intercepts remain the same, but for other points:}\\
\begin{itemize}
   \item[\textcolor{RAL3003}{\textbf{•}}] $2f(x)$ graph moves \textbf{farther} from $x$-axis.
   \item[\textcolor{RAL3003}{\textbf{•}}] $\frac{1}{2}f(x)$ graph moves \textbf{closer} to $x$-axis.
\end{itemize}

\begin{center}
   \begin{tabular}{c|c|c}
       \begin{tikzpicture}[scale=1]
           \begin{axis}[
               axis lines=middle,
               xlabel={$x$},
               ylabel={$y$},
               xtick=\empty,
               ytick=\empty,
               xmin=-3, xmax=4,
               ymin=-2, ymax=5,
               domain=-3:3,
               samples=100,
               smooth,
               title style = {font=\boldmath},
               title={$y=f(x)$},
               width=4cm,
               height=4cm
           ]
               \addplot[RAL3003, line width=2pt] {(x)^2};
           \end{axis}
       \end{tikzpicture}
       &
         \begin{tikzpicture}[scale=1]
               \begin{axis}[
                   axis lines=middle,
                   xlabel={$x$},
                   ylabel={$y$},
                   xtick=\empty,
                   ytick=\empty,
                   xmin=-3, xmax=4,
                   ymin=-2, ymax=5,
                   domain=-3:3,
                   samples=100,
                   smooth,
                   title style = {font=\boldmath},
                   title={$y=2f(x)$},
                   width=4cm,
                   height=4cm
       ]
           \addplot[RAL3003, line width=2pt] {2*(x)^2};
           \addplot[line width=1.5pt, dashed] {(x)^2};
       \end{axis}
   \end{tikzpicture}
        & \begin{tikzpicture}[scale=1]
               \begin{axis}[
                   axis lines=middle,
                   xlabel={$x$},
                   ylabel={$y$},
                   xtick=\empty,
                   ytick=\empty,
                   xmin=-3, xmax=4,
                   ymin=-2, ymax=5,
                   domain=-3:3,
                   samples=100,
                   smooth,
                   title style = {font=\boldmath},
                   title={$y=5f(x)$},
                   width=4cm,
                   height=4cm
       ]
           \addplot[RAL3003, line width=2pt] {5*x^2};
           \addplot[line width=1.5pt, dashed] {(x)^2};
       \end{axis}
   \end{tikzpicture}
   \end{tabular}    
\end{center}

\begin{center}
   \begin{tabular}{c|c}
       \begin{tikzpicture}[scale=1]
           \begin{axis}[
               axis lines=middle,
               xlabel={$x$},
               ylabel={$y$},
               xtick=\empty,
               ytick=\empty,
               xmin=-3, xmax=4,
               ymin=-2, ymax=5,
               domain=-3:3,
               samples=100,
               smooth,
               title style = {font=\boldmath},
               title={$y=\frac{1}{2}f(x)$},
               width=4cm,
               height=4cm
               ]
               \addplot[RAL3003, line width=2pt] {(x)^2/2};
               \addplot[line width=1.5pt, dashed] {(x)^2};
           \end{axis}
       \end{tikzpicture}
   & \begin{tikzpicture}[scale=1]
               \begin{axis}[
                   axis lines=middle,
                   xlabel={$x$},
                   ylabel={$y$},
                   xtick=\empty,
                   ytick=\empty,
                   xmin=-5, xmax=6,
                   ymin=-2, ymax=5,
                   domain=-4:4,
                   samples=100,
                   smooth,
                   title style = {font=\boldmath},
                   title={$y=\frac{1}{3}f(x)$},
                   width=6cm,
                   height=4cm
       ]
           \addplot[RAL3003, line width=2pt] {(x)^2/3};
           \addplot[line width=1.5pt, dashed] {(x)^2};
       \end{axis}
   \end{tikzpicture}
   \end{tabular}    
\end{center}






