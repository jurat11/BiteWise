\newpage







\subsection*{Linear Inequalities}
\addcontentsline{toc}{subsection}{Linear Inequalities}
\renewcommand{\answerlist}{} 
\begin{questions} 
\begin{multicols*}{2} 
   
    \question\textbf{[June 2023]} \\ \\ %easy
        In the $xy$-plane, which of the following does NOT contain any points ($x,y$) that are solutions to $7x+4y>12$?
        \begin{itemize}
                    \item[A)] The region where $x>0$
                    and $y>0$
                    \item[B)] The region where $x<0$ and $y>0$
                    \item[C)] The region where $x<0$ and $y<0$
                    \item[D)] The region where $x>0$ and $y<0$
                \end{itemize}
    \saveanswer{C}
        
    
    \vspace{4cm} \question \textbf{[August US 2023]} \\ %easy
        $$y\leq \frac{2}{3}x+5$$ 
    For which of the following tables are all the values of $x$ and their corresponding values of $y$ solutions to the given inequality?
        \begin{multicols}{2}
            \begin{itemize}
                \item[A)] 
                    \begin{tabular}{|c|c|}
                        \hline
                        $x$ & $y$ \\
                        \hline
                        -1.5 & 6 \\
                        \hline
                        1.5 & 5 \\
                        \hline
                        3 & -4 \\
                        \hline
                    \end{tabular}                % Javoblar kiritiladi
                \item[B)]
                    \begin{tabular}{|c|c|}
                        \hline
                        $x$ & $y$ \\
                        \hline
                        -1.5 & 5 \\
                        \hline
                        1.5 & 7 \\
                        \hline
                        3 & 8 \\
                        \hline
                    \end{tabular}                   
                \item[C)] 
                    \begin{tabular}{|c|c|}
                        \hline
                        $x$ & $y$ \\
                        \hline
                        -1.5 & 6 \\
                        \hline
                        1.5 & 5 \\
                        \hline
                        3 & 6 \\
                        \hline
                    \end{tabular} 
                \item[D)]
                    \begin{tabular}{|c|c|}
                        \hline
                        $x$ & $y$ \\
                        \hline
                        -1.5 & 3 \\
                        \hline
                        1.5 & 5 \\
                        \hline
                        3 & 6 \\
                        \hline
                    \end{tabular}                 
            \end{itemize}    
        \end{multicols}
    \saveanswer{D}


    \vspace{4cm} \question\textbf{[June 2023]} \\ \\ %easy
        In December 2017, the lowest temperature recorded in a certain city was 40 degrees Fahrenheit ($F^\circ$) and the highest temperature recorded was $90^\circ F$. Which inequality is true for all values of $t$, where $t$ represents any temperature in $F^\circ$, recorded in the city in December?
            \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] $40\leq t \leq 90$
                    \item[B)] $t \leq 40$ 
                    \item[C)] $t \leq 50$
                    \item[D)] $t \geq 90$
                \end{itemize}
            \end{multicols}
    \saveanswer{A}  

    
        \vspace{4cm} \question \textbf{[December 2023]} \\ \\ %medium
    A model estimates that a gray whale travels 73 to 78 miles each day during its migration. Based on this model, which inequality represents the estimated total number of miles,$x$, a gray whale could travel in 31 days of its migration?
                \begin{itemize}
                    \item[A)] $(73)(31)\leq{x}\leq(78)(31)$
                    \item[B)] $73+31\leq{x}\leq 78+31$
                    \item[C)] $73\leq{31x}\leq78$
                    \item[D)] $73\leq{31+x}\leq 78$
                \end{itemize}
    \saveanswer{A}
    
        \vspace{3cm} \question \textbf{[December 2023]} \\   %easy
    $$
       \begin{tikzpicture}
            \begin{axis}[
                axis lines=middle,
                grid=both,
                xmin=-9.5, xmax=8.5,
                ymin=-9.5, ymax=8.5,
                xlabel=$x$,
                ylabel=$y$,
                xtick={-8,-6,-4,-2,0,2,4,6,8},
                ytick={-8,-6,-4,-2,0,2,4,6,8},
                width = 8cm, % Width of the plot
                height = 10cm,
                axis line style={-stealth, very thick},
                every axis x label/.style={at={(ticklabel* cs:1)},anchor=west}, % Position xlabel slightly to the right 
                every axis y label/.style={at={(ticklabel*   cs:1)},anchor=south}, % Position ylabel slightly above
                ]
                \addplot[thick, smooth, line width=2pt, domain=0:10]{-5/2*x+13};
                \fill[gray, opacity=0.4] (8,8) -- (8,-7) -- (5,0) -- (2,8) -- cycle;
            \end{axis}
            \end{tikzpicture}$$
            The shaded region shown represents solutions to inequality. Which ordered pair ($x,y$) is a solution to this inequality?
             \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] (-7,0)
                    \item[B)] (0,-7)
                    \item[C)] (0,7)
                    \item[D)] (7,0)
                \end{itemize}
        \end{multicols}
    \saveanswer{D}
    
        \vspace{3cm} \question \textbf{[March 2024]} \\ \\ The minimum value of $x$ is 11 less than 8 times another number $n$. Which inequality shows the possible values of $x$?
        \begin{itemize}
            \item[A)] $ x \leq 8n - 11 $
            \item[B)] $ x \geq 8n - 11 $
            \item[C)] $ x \leq 11 - 8n $
            \item[D)] $ x \geq 11 - 8n $
        \end{itemize}
