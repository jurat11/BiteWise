    \vspace{0.2cm}   \vspace{6cm} \question\textbf{[June 2024]}\saveanswer{A} \\ \ \ \\
        \begin{tikzpicture}
            \begin{axis}[
                axis lines=middle,
                grid=both,
                xmin=-6.3, xmax=6.3,
                ymin=-6.3, ymax=6.3,
                xlabel=$x$,
                ylabel=$y$,
                xtick={-6,-4,-2,0,2,4,6},
                ytick={-6,-4,-2,0,2,4,6},
                minor xtick={-5,-3,-1,1,3,5},
                minor ytick={-5,-3,-1,1,3,5},
                width = 8cm, % Width of the plot
                height = 8cm,
                axis line style={-stealth, very thick},
                every axis x label/.style={at={(ticklabel* cs:1)},anchor=west}, % Position xlabel slightly to the right 
                every axis y label/.style={at={(ticklabel*   cs:1)},anchor=south}, % Position ylabel slightly above
                ]
                \addplot[thick, smooth, line width=2pt, domain=-2:6]{3-3/2*x};
            \end{axis}
        \end{tikzpicture}
