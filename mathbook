                    \begin{axis}[
                        axis lines=middle,
                        grid=both,
                        xmin=-1, xmax=11,
                        ymin=-1, ymax=7,
                        xlabel=$x$,
                        ylabel=$y$,
                        xtick={0,2,4,6,8,10},
                        ytick={0,2,4,6},
                        minor xtick={1,3,5,7,9}, % Minor ticks (odd numbers without labels)
                        minor ytick={1,3,5},
                        width = 7cm, % Width of the plot
                        height = 5cm,
                        ]
