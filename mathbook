\begin{questions}
    \begin{multicols*}{2}

 \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{A} \\ \\
        If $(x+9)-5=25$, what is the value of $x+9$?
        \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] 30
                    \item[B)] 21
                    \item[C)] 20
                    \item[D)] 11
                \end{itemize}
        \end{multicols}
   \vspace{4cm} \question\textbf{[March US 2023]}\saveanswer{A} \\ \\
        Which equation has no solution?
        \begin{itemize}
                    \item[A)] $x=x+1$
                    \item[B)] $x=2x+2$
                    \item[C)] $x+1=x+1$
                    \item[D)] $x+1=2x+2$
                \end{itemize}
  \vspace{3cm} \question\textbf{[June 2023]}\saveanswer{B} \\ \\
    If $5(x+1)=25$, what is the value of $x+1$?
        \begin{multicols}{2}
            \begin{itemize}
                \item[A)] 4
                \item[B)] 5
                \item[C)] 19
                \item[D)] 20
            \end{itemize}
        \end{multicols}
    
      \vspace{4cm} \question\textbf{[March US 2023]}\saveanswer{D} \\
                    $$|15-x|-9=0$$
        What are all positive solutions to the given equation?
                    \begin{itemize}
                        \item[A)] $-6$ and $-24$
                        \item[B)] $-6$ and $24$
                        \item[C)] $6$ and $-24$
                        \item[D)] $6$ and $24$
                    \end{itemize}
                    
                    
    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{A} \\
                         $$ax-y=bxy$$
             The given equation relates the positive numbers $a$,$b$,$x$,and $y$. Which equation correctly expresses $a$ in terms of $b$, $x$,and $y$?
                    \begin{itemize}
                    \item[A)] $a=\dfrac{bxy+y}{x}$
                    \item[B)] $a=\dfrac{bxy-y}{x}$
                    \item[C)] $a=\dfrac{x}{bxy+y}$
                    \item[D)] $a=\dfrac{x}{bxy-y}$
                \end{itemize}
                
    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{B} \\ \\
           What value of $x$ is the solution to the equation $x+10=3$
            \begin{multicols}{2}
            \begin{itemize}
                    \item[A)] -13
                    \item[B)] -7
                    \item[C)] 7
                    \item[D)] 13
                \end{itemize}
            \end{multicols} 


