            \end{itemize}

    \vspace{0.2cm}   \vspace{2cm} \question \textbf{[March 2024]}\saveanswer{D} \\ \\ Which system of linear equations has no solution?
        \begin{itemize}
            \item[A)] $-2x + 8y = -5$ \\
                      $2x - 8y = 5$
            \item[B)] $2x - 8y = 5$ \\
                      $3x + 9y = 6$
            \item[C)] $2x - 8y = 5$ \\
                      $-6x + 24y = -15$
            \item[D)] $-2x + 8y = 5$ \\
                      $4x - 16y = 10$
        \end{itemize}

