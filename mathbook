\begin{itemize}
    \item[A)] $d = \dfrac{c}{7(2 - 5c)}$
    \item[B)] $d = \dfrac{7c}{2 - 5c}$
    \item[C)] $d = \dfrac{7(2 - 5c)}{c}$
    \item[D)] $d = \dfrac{2 - 5c}{7c}$
\end{itemize}


