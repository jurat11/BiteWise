        \begin{itemize}
            \item[A)] $1.87c + 2.37t \leq 20$
            \item[B)] $1.87c + 2.37t \geq 20$
            \item[C)] $2.37c + 1.87t \leq 20$
            \item[D)] $2.37c + 1.87t \geq 20$
        \end{itemize}

       
