\vspace{0.2cm}   \vspace{6cm} \question\textbf{[March 2024]}\saveanswer{A} \\ 
    $$
    \begin{tikzpicture}
    \begin{axis}[
        axis lines = middle,
        grid = both,
        ymin = -13.5, ymax = 0,
        xmin = -15, xmax = 0,
        xtick = {0,-2,-4,-6,-8,-10,-12,-14,-16},
        ytick = {0, -1,-3,-5,-7,-9,-11,-13},
        width = 8cm,
        height = 8cm,
        every axis x label/.style = {at={(ticklabel* cs:1)}, anchor=north},
        every axis y label/.style = {at={(ticklabel* cs:1)}, anchor=south},
    ]
        % Plotting the line
        \addplot[
            domain = -15:0,
            samples = 100,
            thick,
        ] {(-13/14)*x - 13};
        
        % Adding the first point
        \addplot[
            only marks,
            mark=*,
            mark size=3pt,
            color=black
        ] coordinates {(0, -13)};
        
        % Adding the second point
        \addplot[
            only marks,
            mark=*,
            mark size=3pt,
            color=black
        ] coordinates {(-14, 0)};
    \end{axis}
\end{tikzpicture}
$$
