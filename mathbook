\subsection*{Exponents\&Radicals}
\addcontentsline{toc}{subsection}{Exponents\&Radicals} 

\begin{questions}
    \begin{multicols*}{2}
    
    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{B} \\
            $$\sqrt{a^2 b^6}$$          
    If $a$ and $b$ are positive numbers, which of the following is equivalent to the expression above?
    \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] $ab^4$
                    \item[B)] $ab^3$
                    \item[C)] $ab{\sqrt{ab^4}}$
                    \item[D)] $ab{\sqrt{b^3}}$ 
                \end{itemize}
        \end{multicols}
    \vspace{0.2cm}   \vspace{2cm} \question\textbf{[March US 2023]}\saveanswer{A} \\ \\
        Which expression is equivalent to {\normalsize$\sqrt[5]{32a^3b^4}$, where $a>0$ and $b>0$?}
        \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] {\large$2$$a^{\frac{3}{5}}$$b^{\frac{4}{5}}$}
                    \item[B)] {\large$2$$a^{\frac{5}{3}}$$b^{\frac{5}{4}}$}
                    \item[C)] {\large$32$$a^{\frac{3}{5}}$$b^{\frac{4}{5}}$}
                    \item[D)] {\large$32$$a^{\frac{5}{3}}$$b^{\frac{5}{4}}$}
                \end{itemize}
                \end{multicols}
    \vspace{0.2cm}   \vspace{2cm} \question\textbf{[June 2023]}\saveanswer{C} \\ \\
        The expression $${\frac{x^{20}(x-4)}{5x^2}}+{\frac{4x^{20}}{5x^2}}$$ is equivalent to {\large ${\frac{1}{5}}x^c$}, where c is a constant and $x>0$. What is the value of $c$?
            \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] 4
                    \item[B)] 5
                    \item[C)] 19
                    \item[D)] 21
                \end{itemize}
        \end{multicols}
        
        \vspace{0.2cm}   \vspace{3cm} \question \textbf{[August US 2023]}\saveanswer{A} \\ $$W=\frac{R^2}{R^2+S^2}$$  
            The given equation relates the variables $R$, $S$, and $W$, where $R$, $S$, and $W$ represent positive numbers. Which equation correctly expresses $S$ in terms of $R$ and $W$?% Savol kiritiladi
                \begin{itemize}
                    \item[A)] $S=\sqrt{\frac{R^2-WR^2}{W}}$ % Javoblar kiritiladi
                    \item[B)] $S=\frac{R^2}{R^2+W^2}$
                    \item[C)] $S=\sqrt{R^2-WR^2}$
                    \item[D)] $S=\sqrt{\frac{R^2}{W}}-R^2$
                \end{itemize}    

    \vspace{0.2cm}   \vspace{4cm} \question\textbf{[March 2024]}\saveanswer{C} \\
        $$f(x)=9x-7$$
        Function $f$ is defined by the given equation. The function $g$ is defined by $$g(x)=f(x)-(7x-2).$$ Whats is the $x$-coordinate of the $x$-intercept of the graph of $y=g(x)$ ing the $xy$-plane?
        \begin{multicols}{2}
            \begin{itemize}
            \item[A)] \(\dfrac{9}{16}\)
            \item[B)] \(\dfrac{7}{9}\)
            \item[C)] \(\dfrac{5}{2}\)
            \item[D)] \(\dfrac{9}{2}\)
        \end{itemize}
        \end{multicols}

    \vspace{0.2cm}   \vspace{4cm} \question\textbf{[May US 2024]}\saveanswer{7/6} \\ \\
    If $n$ and $k$ are numbers greater than 1 and $\sqrt[3]{n^4}$ is equivalent to $\sqrt[5]{k^2}$, for what value of $a$ is $n^{2a+1}$ equal to $k$?

    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[June 2024]}\saveanswer{4/225} \\ $$\sqrt[5]{119n}\left(\sqrt[6]{119n}\right)^2$$
        For what value of $x$ is the given expression equivalent to $(119n)^{30x},$ where $n>1?$

\vspace{0.2cm}   \vspace{3cm} \question \textbf{[June 2024]}\saveanswer{23/30}\\ $$\sqrt[5]{p^2} = t^{\frac{3}{4}}$$
In the given equation, $p > 1$ and $t > 1$. If $t = p^{2n-1}$ where $n$ is a constant, what is the value of $n$?

\vspace{0.2cm}   \vspace{3cm} \question \textbf{[August 2024]}\saveanswer{B}\\ \\ Which expression is equivalent to $(\sqrt[12]{x^5y^5})$, where $x$ and $y$ are positive?
    \begin{multicols}{2}
        \begin{itemize}
        \item[A)] $(xy)^{\frac{12}{5}}$
        \item[B)] $(xy)^{\frac{5}{12}}$
        \item[C)] $(xy)^{17}$
        \item[D)] $(xy)^{60}$
    \end{itemize}
    \end{multicols}

\vspace{0.2cm}   \vspace{3cm} \question\textbf{[August US 2024]}\saveanswer{18} \\ \\
The expression $(6x^{2})(3x^{7})$ is equivalent to $bx^{9}$, where $b$ is a constant. What is the value of $b$?

    \vspace{0.2cm}   \vspace{2cm} \question\textbf{[October US 2024]}\saveanswer{C} \\ 
        $$r=\sqrt{5w-3}$$
        The given equation relates the real number $r$ and $w$, where $w>\frac{3}{5}$. Which equation correctly expresses $w$ in terms of $r$?
        \begin{itemize}
            \item [A)]$w=\sqrt{-3r+5}$
            \item [B)]$w=\sqrt{5r-3}$
            \item [C)]$w=\dfrac{r^2}{5}+\dfrac{3}{5}$
            \item [D)]$w=\dfrac{r^2}{5}+3$
        \end{itemize}

    \vspace{0.2cm}   \vspace{3.5cm} \question \textbf{[December US 2024]}\saveanswer{7/16} \\ \\ If $n$ and $k$ are numbers greater than 1 and $\sqrt[4]{n^5}$ is equivalent to $\sqrt[3]{k^2}$, for what value of $a$ is $n^{2a+1}$ equal to $k$?

    \vspace{0.2cm}   \vspace{5.5cm} \question \textbf{[December 2024]}\saveanswer{5/7} \\ $$\sqrt[5]{x^m}$$
    In the given expression, $m$ is a constant and $x > 1$. The expression can be rewritten as $ \sqrt[7]{x} $. What is the value of $m$?

    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March 2025]}\saveanswer{C} \\ \\ 
    Which expression is equivalent to $(158y)^{\frac{1}{2}}$, where $y > 1$?
    \begin{multicols}{2}
    \begin{itemize}
        \item[A)] $158 \cdot \sqrt{y}$
        \item[B)] $\sqrt{158} \cdot y$
        \item[C)] $\sqrt{158y}$
        \item[D)] $\sqrt{(158y)^2}$
    \end{itemize}
    \end{multicols}

\vspace{0.2cm}   \vspace{2cm} \question \textbf{[June US 2025]}\saveanswer{B}\\ \\
$$ p = \sqrt{12c - 25} $$

The given equation relates the variables $p$ and $c$, where $c$ is positive. Which equation correctly expresses $c$ in terms of $p$?
\begin{itemize}
    \item[A)] $c = \dfrac{(p + 25)^2}{12}$
    \item[B)] $c = \dfrac{p^2}{12} + \dfrac{25}{12}$
    \item[C)] $c = \dfrac{p}{24} + \dfrac{25}{24}$
    \item[D)] $c = \dfrac{\sqrt{p + 25}}{12}$
\end{itemize}




    
                
        
    \end{multicols*}
\end{questions}
