            \draw[thick,line width=1.5pt] (-1, -1.5) -- (1, 4) node[above] {$a$}; % Diagonal line a
            \draw[thick,line width=1.5pt] (-2, 0) -- (4, 0) node[right] {$e$}; % Diagonal line b
            % Label points
