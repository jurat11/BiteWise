                xtick={-4, -2, 2, 4},
                ytick={2, 4, ..., 10},
                minor xtick={-5, -3, ..., 5},
                minor ytick={-1,1,3,5,7,9},
                width = 8cm,
                height = 8cm,
                axis line style={-stealth, very thick},
                every axis x label/.style={at={(ticklabel* cs:1)},anchor=west}, 
                every axis y label/.style={at={(ticklabel*   cs:1)},anchor=south},
                ]
                \draw[domain = -5:2, smooth, thick, line width=1.5pt] plot (\x, {-2*\x+1}) node[right] {};
                \node at (-0.5,-0.5) {$O$};
            \end{axis}
        \end{tikzpicture}
        $$
