\newpage

\thispagestyle{empty}  % Remove header and footer (optional)

\vspace*{\fill}  % Push content to the bottom
\begin{center}
    {\textcolor{RAL3003}{\scalebox{7}{\makecell{\textbf{Problem}\\ \textbf{solving}}}}}  % Scale text 4x larger
\end{center}
\vspace*{\fill}
\addcontentsline{toc}{section}{Problem solving} 


\newpage
\subsection*{Problem Solving Formulas}
\addcontentsline{toc}{subsection}{Problem solving Formulas} 


\begin{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Mean (Average)} =  \( \dfrac{\text{Sum of values}}{\text{Total number of values}} \).
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Mode -} The most frequently occurring value.
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Range -} The difference between the highest and lowest values in a dataset.  \textbf{max-min} 
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Median -} The middle value in a sorted dataset, which divides the data into two equal halves.
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Low standard deviation:} Data points are close to the mean.
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{High standard deviation:} Data is spread over a wide range.
    
    \item[\textcolor{RAL3003}{\textbf{•}}]\textbf{Margin of Error:} How It Changes
    \item[] \textbf{Decreases When:}
          \begin{itemize}
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Bigger Sample Size:} More data reduces uncertainty.
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Variability Decreases:} Less spread in the data narrows the error range.
          \end{itemize}
    \item[] \textbf{Increases When:}
          \begin{itemize}
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Smaller Sample Size:} More data increases uncertainty.
            \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Variability Increases:} More spread in the data enlarges the margin.
          \end{itemize}
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Probability} =  \( \dfrac{\text{desired outcomes}}{\text{total possible outcomes}} \).
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Percent Change=}$\dfrac{\text{new$-$old}}{\text{old}}\cdot100\%.$
    \item[\textcolor{RAL3003}{\textbf{•}}] \textbf{Box Plot -} a way of summarizing data by showing:
        \begin{itemize}
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{minimum value} (left whisker).
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{lower quartile} ($Q_1$), which is the 25\% point.
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{median} (50\% point).
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{upper quartile} ($Q_3$), which is the 75\% point.
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{maximum value} (right whisker).
          \item[\textcolor{RAL3003}{\textbf{•}}] The \textbf{Interquartile Range (IQR)} which is $Q_3 - Q_1$.
        \end{itemize}
        \end{itemize}
        $$
        \begin{tikzpicture}[scale=2.0,line width=1.5pt]
        
        % Define some custom colors (optional)
        \definecolor{myblue}{RGB}{0,0,255}
        \definecolor{myred}{RGB}{255,0,0}
        \definecolor{mygreen}{RGB}{0,128,0}
        \definecolor{myorange}{RGB}{255,165,0}
        \definecolor{mybox}{RGB}{245,245,220} % light beige
        \definecolor{myviolet}{RGB}{148,0,211}
        
        % Whisker from min to Q1
        \draw[thick, mygreen,line width=1.5pt] (0,0) -- (1,0);
        \node[below, black] at (0,0) {\textbf{min}};
        
        % Box background
        \fill[mybox] (1,-0.5) rectangle (3,0.5);
        \draw[thick, black,line width=1.5pt,line width=1.5pt] (1,-0.5) rectangle (3,0.5);
        
        % Q1 line
        \draw[thick, myblue,line width=1.5pt] (1,-0.6) -- (1,0.6);
        \node[below, myblue] at (1,-0.7) {$Q_1$};
        
        % Median line
        \draw[thick, myred,line width=1.5pt] (2,-0.6) -- (2,0.6);
        \node[below, myred] at (2,-0.7) {Median};
        
        % Q3 line
        \draw[thick, myorange,line width=1.5pt] (3,-0.6) -- (3,0.6);
        \node[below, myorange] at (3,-0.7) {$Q_3$};
        
        % Whisker from Q3 to max
        \draw[thick, mygreen,line width=1.5pt] (3,0) -- (4,0);
        \node[below, black] at (4,0) {\textbf{max}};
        
        % IQR arrow
        \draw[<->, thick, myviolet,line width=1.5pt] (1,-1.2) -- (3,-1.2);
        \node[below, myviolet] at (2,-1.3) {\textbf{IQR} = $Q_3 - Q_1$};
        
        \end{tikzpicture}
       % \caption{Box plot showing min, $Q_1$, median, $Q_3$, max, and the IQR.}
       $$


\newpage



\newpage
\renewcommand{\answerlist}{}

\subsection*{Percent; Ratio\&Proportion}
\addcontentsline{toc}{subsection}{Percent; Ratio\&Proportion}





\begin{questions}
    \begin{multicols*}{2}
    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{D} \\ \\       
    If $x>0$ and $p\%$ of $x$ is 13, which expression represents $x$ in terms of $p$
        \begin{multicols}{2}
                \begin{itemize}
                    \item[A)] $13p$
                    \item[B)] {\large${\frac{13p}{100}}$}
                    \item[C)] {\large${\frac{100p}{13}}$}
                    \item[D)] {\large ${\frac{(100)(13)}{p}}$}
                \end{itemize}
        \end{multicols}
    \vspace{0.2cm}   \vspace{3cm} \question\textbf{[March US 2023]}\saveanswer{B} \\ \\
