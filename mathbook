                \item[A)] $4$
                \item[B)] $14$
                \item[C)] $50$
                \item[D)] $70$
            \end{itemize}
        \end{multicols}

       \vspace{5cm} \newcolumn \question\textbf{[May US 2024]}\saveanswer{C} \\ \\
