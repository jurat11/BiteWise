\begin{itemize}
    \item [A)]$-\dfrac{84}{13}$
    \item [B)]\(\dfrac{13}{14}\)
    \item [C)]\(\dfrac{83}{14}\)
    \item [D)]\(\dfrac{280}{13}\)
\end{itemize}
\end{multicols}
