\newcommand{\answerlist}{}
\newcommand{\saveanswer}[1]{%
  \listgadd{\answerlist}{#1}%
  \ifprintanswers\relax\else\phantom{#1}\fi%
}

% Corrected \printanswertable
\newcommand{\printanswertable}{%
  \setcounter{tablerow}{0} % CHANGED: Reset instead of redefine
  \renewcommand*{\do}[1]{%
    \stepcounter{tablerow}%
    \ifnumodd{\value{tablerow}}
      {\Large \thetablerow & \Large ##1}
      {& \Large \thetablerow & \Large ##1 \\ \hline}%
  }%
  \begin{longtable}{|c|c|c|c|}
    \hline
    \Large \textbf{Number} & \Large \textbf{Answer} & \Large \textbf{Number} & \Large \textbf{Answer} \\ \hline
    \endhead
    \dolistloop{\answerlist}
    \ifodd\value{tablerow}
      & & \\ \hline
    \fi
  \end{longtable}%
}

